<!--登陆页 @author: chh-->
<chh-navigation-bar back title="注册"/>

<!-- 内容 -->
<view class="content">
  <image class="logo" src="/assets/login/logo.png" />

  <view class="input-list">
    <view class="input-item">
      <input type="text" placeholder="用户名" model:value="{{username}}"/>
    </view>
    <view class="input-item">
      <input type="password" placeholder="密码" model:value="{{password}}"/>
    </view>
  </view>

  <!-- 按钮 -->
  <view class="btn" hover-class="btn-hover" bindtap="onRegister">注册</view>
</view>
  

 

  
  

