<!--pages/test/test.wxml-->
<chh-navigation-bar back title="登录"/>

<!-- 内容 -->
<view class="content">
  <image class="logo" src="/assets/login/logo.png" />

  <view class="input-list">
    <view class="input-item">
      <input type="text" placeholder="手机号" model:value="{{tel}}" bindinput="a"/>
    </view>
    <view class="input-item">
      <input type="password" placeholder="身份证号" model:value="{{cartId}}" bindinput="a"/>
    </view>
  </view>

  <!-- 按钮 -->
  <view class="btn" hover-class="btn-hover" bindtap="onLogin">跳转物业缴费</view>
  <!-- 帮助 -->
  <view class="register">
    <text>记住密码</text>
    <checkbox class="checkbox" checked="{{checked}}" bindtap="onCheck" />
    <text bindtap="toUrl" data-url="../register/register">用户注册</text>
  </view>
</view>
  
